  },
  "dependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "tailwindcss": "^2.2.16"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react": "^1.0.0",
    "typescript": "^4.3.2",
    "vite": "^2.6.0"
  }
}
