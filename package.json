{
  "name": "mark_me",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "start": "vite --port 8080",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "tailwind:css": "tailwind build -i src/CSS/tailwind.css -c tailwind.config.js -o src/CSS/index.css",
    "production:tailwind": "NODE_ENV=production npx tailwindcss -i src/CSS/tailwind.css -o src/CSS/index.css",
    "production": "NODE_ENV=production npx tailwindcss -i src/CSS/tailwind.css -o src/CSS/index.css && tsc && vite build"
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.8.0",
    "@heroicons/react": "^1.0.4",
    "react": "^16.8.3",
    "react-barcode-reader": "^0.0.2",
    "react-dom": "^16.8.3",
    "react-weblineindia-qrcode-scanner": "^1.0.0",
    "tailwindcss": "^2.2.16"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.3.1",
    "@vitejs/plugin-react": "^1.0.0",
    "typescript": "^4.3.2",
    "vite": "^2.6.0"
  }
}
